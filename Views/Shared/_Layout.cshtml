<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Meta Description -->
    <meta property="og:image" content="~/imgs/logo.png" />
    <link rel="icon" href="~/imgs/logo.png">
    <meta name="msapplication-TileColor" content="#ffffff" />
    <meta name="theme-color" content="#ffffff" />
    <meta name="keywords" content="Learn, Maths, Mathmatics, Story, School, Homework, Learning, Education, fun" />
    <meta property="og:description"
        content="The best online, free mathmatics learning platform! Summit allows you to learn mathmatics whenever you 
            want and wherever you are! Simply click Start learning to unlock a massive library off free education; Enjoy!" />

    <!-- style Sheets -->
    <link rel="stylesheet" href="https://unpkg.com/boxicons@latest/css/boxicons.min.css">    
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap"
        rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    @* <link rel="stylesheet" href="~/ZA_PLACE.styles.css" asp-append-version="true" /> *@

    <!-- Title -->
    <title>@ViewData["Title"] - ZA_PLACE</title>
</head>
<body>
    <!-- Header -->
    <header>
        <a href="#" class="logo">
            <img src="~/imgs/logo.png" alt="logo">
        </a>

        <ul class="navbar">
            <li><a href="#Home">Home</a></li>
            <li><a href="#Course">Course</a></li>
            <li><a href="#News">News</a></li>
            <li><a href="#AboutUS">About Us</a></li>
            <li><a href="#ContactUs">Contact Us</a></li>
        </ul>

        <div class="header-icons">
            <a asp-area="Identity" asp-page="/Account/Login"><i class="bx bx-user"></i></a>
            <div class="bx bx-menu" id="menu-icon"></div>
        </div>
    </header>
    <!-- Body -->
    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <!-- Footer -->
    <footer>
        <div class="main-contact">
            <div class="contact-content">
                <img src="~/imgs/logo.png" alt="logo">
                <li><a href="#">Facebook</a></li>
                <li><a href="#">Instagram</a></li>
                <li><a href="#">Twitter</a></li>
            </div>
            <div class="contact-content">
                <li><a href="#Home">Home</a></li>
                <li><a asp-controller="Pages" asp-action="AboutUsExtra">Course</a></li>
                <li><a asp-controller="News" asp-action="NewsPage">News</a></li>
                <li><a asp-controller="Pages" asp-action="AboutUsExtra">About Us</a></li>
                <li><a asp-controller="Pages" asp-action="ContactUsExtra">Contact Us</a></li>
            </div>
            <div class="contact-content">
                <li><a href="login">Profile</a></li>
                <li><a href="login">Sign In</a></li>
                <li><a href="login">Sign Up</a></li>
                <li><a href="login">Instructor</a></li>
                <li><a href="login">Student</a></li>
            </div>
            <div class="contact-content">
                <li><a href="#">Egypt Cairo, <br> فيلا ٢٩٦ التجمع الخامس المستثمرين الجنوبيين كمبوند الدفاع الوطني</a></li>
                <li><a href="#">ZaPlace.edu.cent@gmail.com</a></li>
                <li><a href="#">+20 100 747 1007</a></li>
            </div>
        </div>
        <div class="last-text">
           <p>&copy; All rights reserved. 2024 Designed By Faris Hassan</p>
        </div>
    </footer>
    <!-- Scripts -->
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
    </body>
</html>
